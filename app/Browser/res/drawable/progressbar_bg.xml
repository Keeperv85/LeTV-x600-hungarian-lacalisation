<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ffe4e4e4" android:endColor="#ffe4e4e4" android:angle="270.0" android:centerY="2.0" android:centerColor="#ffe4e4e4" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ff8f85f6" android:endColor="#ff8f85f6" android:angle="270.0" android:centerY="2.0" android:centerColor="#ff8f85f6" />
            </shape>
        </clip>
    </item>
</layer-list>