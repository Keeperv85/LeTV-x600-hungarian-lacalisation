    <CheckBoxPreference android:title="@string/pref2_adblock" android:key="adblock" android:defaultValue="true" />
    <CheckBoxPreference android:title="@string/pref2_adblock_toast" android:key="adblock_toast" android:defaultValue="true" />
    <PreferenceCategory android:title="@string/pref_general_autofill_title">
        <CheckBoxPreference android:title="@string/pref_autofill_enabled" android:key="autofill_enabled" android:summary="@string/pref_autofill_enabled_summary" android:defaultValue="true" />
    </PreferenceCategory>
</PreferenceScreen>