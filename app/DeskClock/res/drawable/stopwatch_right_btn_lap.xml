<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/stopwatch_right_lap_disabled" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:width="1.0px" android:color="#ffcccccc" />
            <solid android:color="@color/stopwatch_right_lap_pressed" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval">
            <stroke android:width="1.0px" android:color="#ffcccccc" />
            <solid android:color="@color/stopwatch_right_lap_pressed" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:width="1.0px" android:color="#ffcccccc" />
            <solid android:color="@color/stopwatch_right_lap_normal" />
        </shape>
    </item>
</selector>