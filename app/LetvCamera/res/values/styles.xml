<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.ProxyLauncher" parent="@android:style/Theme.Translucent.NoTitleBar" />
    <style name="Animation.Drawer" parent="@android:style/Animation.Translucent">
        <item name="android:windowEnterAnimation">@anim/drawer_enter</item>
        <item name="android:windowExitAnimation">@anim/drawer_exit</item>
    </style>
    <style name="LauncherTheme" parent="@android:style/Theme.Holo">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/Animation.Drawer</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
    <style name="Theme.Camera" parent="@style/Theme.CameraBase">
        <item name="android:colorBackground">@android:color/black</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@android:color/black</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Theme.CameraBase" parent="@android:style/Theme.Holo" />
    <style name="OnScreenHintTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/primary_text_dark</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">#ff808080</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="OnScreenHintTextAppearance.Small" parent="@style/OnScreenHintTextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/secondary_text_dark</item>
    </style>
    <style name="Animation_OnScreenHint">
        <item name="android:windowEnterAnimation">@anim/on_screen_hint_enter</item>
        <item name="android:windowExitAnimation">@anim/on_screen_hint_exit</item>
    </style>
    <style name="OnViewfinderLabel">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/bg_text_on_preview</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="PanoCustomDialogText">
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
    </style>
    <style name="EffectSettingGrid">
        <item name="android:paddingBottom">3.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/setting_item_list_margin</item>
        <item name="android:layout_marginRight">@dimen/setting_item_list_margin</item>
        <item name="android:horizontalSpacing">3.0dip</item>
        <item name="android:verticalSpacing">3.0dip</item>
        <item name="android:numColumns">3</item>
        <item name="android:choiceMode">singleChoice</item>
    </style>
    <style name="EffectSettingItem">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/setting_picker</item>
        <item name="android:paddingLeft">2.0dip</item>
        <item name="android:paddingTop">9.0dip</item>
        <item name="android:paddingRight">2.0dip</item>
        <item name="android:paddingBottom">9.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearance.DialogWindowTitle" parent="">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">@color/holo_blue_light</item>
    </style>
    <style name="TextAppearance.Medium" parent="@android:style/TextAppearance.Medium" />
    <style name="PopupTitleText_xlarge">
        <item name="android:textSize">@dimen/popup_title_text_size</item>
        <item name="android:textColor">@color/popup_title_color</item>
        <item name="android:layout_gravity">center|left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="PanoCustomDialogText_xlarge">
        <item name="android:textAppearance">@android:style/TextAppearance.Large</item>
    </style>
    <style name="ViewfinderLabelLayout_xlarge">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_margin">13.0dip</item>
    </style>
    <style name="SwitcherButton">
        <item name="android:background">@drawable/bg_pressed_exit_fading</item>
        <item name="android:layout_width">@dimen/switcher_size</item>
        <item name="android:layout_height">@dimen/switcher_size</item>
    </style>
    <style name="MenuIndicator">
        <item name="android:enabled">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">center</item>
    </style>
    <style name="CameraControls">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="UndoBar">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@drawable/panel_undo_holo</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:layout_marginLeft">4.0dip</item>
        <item name="android:layout_marginRight">4.0dip</item>
    </style>
    <style name="UndoBarTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="UndoBarSeparator">
        <item name="android:background">@color/gray</item>
        <item name="android:paddingRight">12.0dip</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:layout_marginBottom">10.0dip</item>
    </style>
    <style name="UndoButton">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/bg_pressed</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:drawablePadding">8.0dip</item>
    </style>
    <style name="PanoViewHorizontalBar">
        <item name="android:background">#ff000000</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="MoreSettingPopup">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="beauty_level">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ModeTextView">
        <item name="android:textSize">@dimen/mode_text_size</item>
        <item name="android:textColor">@drawable/mode_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ModeTextViewWidget">
        <item name="android:textSize">@dimen/mode_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">@dimen/widget_mode_text_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="NumberProgressBar_Default">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:max">120</item>
    </style>
    <style name="CameraAlertDialogButton">
        <item name="android:textSize">@dimen/camera_alert_dialog_button_text_size</item>
        <item name="android:textColor">@color/camera_alert_dialog_button_text</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/alert_dialog_button_background</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
</resources>
