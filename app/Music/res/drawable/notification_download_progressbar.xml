            <gradient android:startColor="@color/notification_download_progress_bg" android:endColor="@color/notification_download_progress_bg" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/notification_download_progress" android:endColor="@color/notification_download_progress" />
            </shape>
        </clip>
    </item>
</layer-list>