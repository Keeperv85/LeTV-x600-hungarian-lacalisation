<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff45b218" />
            <corners android:radius="1.0dip" />
            <stroke android:width="1.0px" android:color="#ff429e24" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff429e24" />
            <corners android:radius="1.0dip" />
            <stroke android:width="1.0px" android:color="#ff3e8d29" />
        </shape>
    </item>
</selector>